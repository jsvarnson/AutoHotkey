;----- email 1 to clip -----	
TMemailtoClip1:
{{
	clipboard := ""	
	clipboard := "joseph.arnson@millercoors.com"
	clipwait
	tooltip, joseph.arnson@millercoors.com copied to clipboard
	SetTimer, TMRemoveToolTip, -2000
	return
}}
