	TMsetWid = 250		; gui settings width
	TMBtnW  := 80 		; button width
	TMedtW  := 70 		; Edit width
	TMBtnX1 := ((TMsetWid - TMBtnW) / 4) * 1
	TMBtnX2 := ((TMsetWid - TMBtnW) / 4) * 3
