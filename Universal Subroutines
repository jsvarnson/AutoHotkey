;=======================================================================
;	Universal Timer
;=======================================================================

; AdjTime = 3595000 	; changes hours	; starts at 1 hour?? fix

;------ UGUI Button Starts Timer ---------------------------------------
U_Timer_Shuttle:
	gosub, U_Timer_LAUNCH
	;gosub, U_Timer_Start
	return
;------ Timer Start ----------------------------------------------------
U_Timer_LAUNCH:
	Menu, Tray, NoStandard                                ; clear the tray menu for customizing
	Menu, Tray, Add, Quit, U_Timer_Exit                        ; add item "Quit" to call AppExit
	Menu, Tray, Default, Quit                             ; double-click on the tray icon will call "Quit"
	Menu, Tray, Tip, % " AHK short-time Stopwatch "       ; add a nice tool tip 
	Gui , Timer:Color, White
	Gui , Timer:+LastFound -Caption +AlwaysOnTop              ; +ToolWindow
	WinSet, TransColor, White 150 
	Gui , Timer:Margin, 0, 0                                     ; http://l.autohotkey....ui.htm#Position
	Gui , Timer:Font, S42 cDefault , Inconsolata
	Gui , Timer:Add , Text , Center vDisplayTime x+16 y+16 w250 , 00:00:00:000
	Gui , Timer:Font, S12, TUNO
	Gui , Timer:Add , Button , x400 yp w75 h60 gU_Timer_Stop Section , STOP
	Gui , Timer:Add , Button , x400 yp w75 h60 gU_Timer_Start, START
	Gui , Timer:Add , Button , x+0 yp w75 h60 gU_Timer_Copy, COPY
	Gui , Timer:Add , Button , x+0 yp w75 h60 gU_Timer_Reset , RESET
	Gui , Timer:Add , Button , x+0 yp w75 h60 gU_Timer_Exit , QUIT
	Gui , Timer:Show , x-862	y162 w734	h94 , Timer
	Control , Hide , , Button1 , Timer
	gosub, U_Timer_Start
	Exit
;------ Timer Exit -----------------------------------------------------
U_Timer_Exit:
	;gui, Timer:Submit
	gui, Timer:Destroy
	return
;------ Timer Start ---------------------------------------------------- 
U_Timer_Start:
	AdjTime = 0
	Control , Hide , , Button2 , Timer
	Control , Show , , Button1 , Timer
	GuiControl , Focus , Button1
	StopClock = 0	
	ControlGetText , DisplayTime , Static1
	ST := A_TickCount ; Start Time
	While StopClock = 0
		ControlSetText 	, Static1 , % FormatCT((A_TickCount - ST)) ; + AdjTime)
	StopClock = 0
	Exit
;------ Timer Stop Reset -----------------------------------------------
U_Timer_Stop:
U_Timer_Reset:
	Control , Show , , Button2 , Timer
	Control , Hide , , Button1 , Timer
	GuiControl , Focus , Button2
	StopClock = 1
	IF (A_ThisLabel = "U_Timer_Reset")
	{
		AdjTime = 0
		ControlSetText , Button2 , START
		ControlSetText 	, Static1 , 00:00:00:000
	}else{
		AdjTime := ((A_TickCount - ST)) ; + AdjTime)
		ControlSetText , Button2 , COUNT
	}
	Exit
;------ Timer Copy -----------------------------------------------------
U_Timer_Copy:
	ControlGetText , Clipboard , Static1
	MsgBox, 262192, Time Copied to Clipboard, Copied to Clipboard: %Clipboard%, 1
	Exit
;------ Timer Close ----------------------------------------------------
TimerGuiClose:
	ExitApp
;------ Format Millisecond ---------------------------------------------
FormatCT(ms)  ;  Formats milliseconds into 00:00:00:000 (last three digits are milliseconds)
{
	StringRight , mil , ms , 3
	StringTrimRight , sec , ms , 3
	min := Floor(sec/60)
	sec := sec-(min*60)
	hrs := Floor(min/60)
	min := min-(hrs*60)
	While StrLen(sec) <> 2
		sec := "0" . sec
	While StrLen(mil) <> 3
		mil := "0" . mil
	While StrLen(min) <> 2
		min := "0" . min
	While StrLen(hrs) <> 2
		hrs := "0" . hrs 
	return , hrs . ":" . min . ":" . sec . ":" . mil
}

;=======================================================================
;	Email
;=======================================================================
;--------------------------------------------------
;	Ws Processing Complete Email
;--------------------------------------------------
Ws_Processing_completed_email:
	InputBox, WsDoneNumCompleted, Number of Files Completed, , , 300, 100, center, center, , , 2					; InputBox	; default -> 2
	if WsDoneNumCompleted = 1 
	{
		FileSelectFile, WsDoneFile1, 1 2,\\mbsus238\trfshare\Import Reference Materials\Ws Import Shared			; Select file	; 1: File Must Exist	; 2: Path Must Exist
		if ErrorLevel   ; cancel run if inputbox is cancelled
			exitapp
		SplitPath, WsDoneFile1, , , , WsDoneFile2																	; SplitPath, InputVar [, OutFileName, OutDir, OutExtension, 
		StringRight, WsDoneName1, WsDoneFile2, 17
		StringLeft, WsDoneName2, WsDoneName1, 4
	}
	else if WsDoneNumCompleted = 2 
	{
		FileSelectFile, WsDoneFile1, 1 2,\\mbsus238\trfshare\Import Reference Materials\Ws Import Shared			; Select file	; 1: File Must Exist	; 2: Path Must Exist
		if ErrorLevel   ; cancel run if inputbox is cancelled
			exitapp
		SplitPath, WsDoneFile1, , , , WsDoneFile2																	; SplitPath, InputVar [, OutFileName, OutDir, OutExtension, OutNameNoExt, OutDrive]
		FileSelectFile, WsDoneFile3, 1 2,\\mbsus238\trfshare\Import Reference Materials\Ws Import Shared			; Select file	; 1: File Must Exist	; 2: Path Must Exist
		StringRight, WsDoneName1, WsDoneFile2, 17
		StringLeft, WsDoneName2, WsDoneName1, 4
	}
	else if WsDoneNumCompleted = 3
	{
		FileSelectFile, WsDoneFile1, 1 2,\\mbsus238\trfshare\Import Reference Materials\Ws Import Shared			; Select file	; 1: File Must Exist	; 2: Path Must Exist
		if ErrorLevel   ; cancel run if inputbox is cancelled
			exitapp
		SplitPath, WsDoneFile1, , , , WsDoneFile2																	; SplitPath, InputVar [, OutFileName, OutDir, OutExtension, OutNameNoExt, OutDrive]
		FileSelectFile, WsDoneFile3, 1 2,\\mbsus238\trfshare\Import Reference Materials\Ws Import Shared			; Select file	; 1: File Must Exist	; 2: Path Must Exist
		FileSelectFile, WsDoneFile4, 1 2,\\mbsus238\trfshare\Import Reference Materials\Ws Import Shared			; Select file	; 1: File Must Exist	; 2: Path Must Exist
		StringRight, WsDoneName1, WsDoneFile2, 17
		StringLeft, WsDoneName2, WsDoneName1, 4
	}
	Else
	{
		msgbox, No files completed
		exitapp
	}				
	olMailItem := 0	
	o:= ComObjActive("Outlook.Application").Session()															; connect to outlook ComObject
	MailItem := ComObjActive("Outlook.Application").CreateItem(olMailItem)										; create new email
	MailItem.BodyFormat := 2 																					; olFormatHTML	
	;~ MailItem.To := WsDoneName2 "@millercoors.com`;"															; Add To line recipients 	; commented out to prevent @ mention from including the recipient twice
	MailItem.CC := "importmgmt@millercoors.com`;"
	if (WsDoneFile1 != "")
		MailItem.Attachments.Add(WsDoneFile1)
	if (WsDoneFile3 != "")
		MailItem.Attachments.Add(WsDoneFile3)
	if (WsDoneFile4 != "")
		MailItem.Attachments.Add(WsDoneFile4)
	TodayDateWsDone := A_DDDD . ", " . A_MMM . " " . A_DD . ", " . A_YYYY ;storing pretty version of date
	MailItem.Subject := "Ws Processing Completed on " TodayDateWsDone 
	MailItem.Display 																							; Make email visible
	sleep, 100
	send, {tab 4}
	sleep, 100
	if (WsDoneNumCompleted = 1)
	{
	send, 
(
Hello @%WsDoneName2%{tab},

I have completed your Winshuttle processing and attached the file here for your convenience. As always, please make sure to review the file for completeness and correctness and let me know if you have any other questions so we may discuss them further. If anything was missed, or you believe something was processed incorrectly, please let me know as soon as possible so we may resolve the issue.

Cheers,
)
		sleep, 100
		return
	}
	else 
	{
		send, 
(
Hello @%WsDoneName2%{tab},

I have completed your Winshuttle processing and attached the files here for your convenience. As always, please make sure to review the files for completeness and correctness and let me know if you have any other questions so we may discuss them further. If anything was missed, or you believe something was processed incorrectly, please let me know as soon as possible so we may resolve the issue.

Cheers,
)
		sleep, 100
		return
	}
;--------------------------------------------------
;	Universal End
;--------------------------------------------------
}}
;====================================================================================================
;	Global
;====================================================================================================	
{{
;-------------------------------------------------------------------------------------
;	Key Mapping
;-------------------------------------------------------------------------------------
/*
Capslock							->  Enter
ctrl + Capslock						->  Toggle Capslock
`									->  Backwards Tab
Numlock Always On
*/
;--------------------------------------------------
;	Capslock as Enter
;--------------------------------------------------
{{
Capslock::
	send, {enter}
	return
}}
;--------------------------------------------------
;	Set Capslock
;--------------------------------------------------	
{{	
^CapsLock::
    GetKeyState, capsstate, CapsLock, T ;(T indicates a Toggle. capsstate is an arbitrary varible name)
    if capsstate = U
        SetCapsLockState, AlwaysOn
    else
        SetCapsLockState, AlwaysOff
    return
}}	
