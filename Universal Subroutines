#Ifwinactive, ahk_class AutoHotkeyGUI
~Esc::
	gosub, Save_Universal_Notes
	Gui, Destroy
	return
#Ifwinactive

~^+Esc::					; Exits all active scripts
	gosub, Save_Universal_Notes
	Gui, Destroy
	Exitapp
	return

~^Esc::					; Pauses all active scripts
	Suspend, on
	Pause, on
	return

~^!#+r::					; Unpauses and reloads all active scripts
	Suspend, off
	Pause, off
	SetTimer,UPDATEDSCRIPT,1000
	return	
;--------------------------------------------------
;	GUI Specific Hotkeys
;--------------------------------------------------
#Ifwinactive, ahk_class AutoHotkeyGUI
~^s::						; Reloads scipt upon save
	Gui, Submit, NoHide
	SetTimer,UPDATEDSCRIPT,1000
	UPDATEDSCRIPT:
		FileGetAttrib,attribs,%A_ScriptFullPath%
		IfInString,attribs,A
		{
			FileSetAttrib,-A,%A_ScriptFullPath%
			Reload
		}
	return

:*:[[::
	TodayDateUniversal1 := A_DDDD . ", " . A_MMMM . " " . A_DD . ", " . A_YYYY . " at " . A_Hour . ":" . A_Min				; gets current date time stamp formatting
	send, %TodayDateUniversal1%
	return
	
:*:]]::
	TodayDateUniversal2 := A_MM . "-" . A_DD . "-" . A_YYYY . " @ " A_Hour . ":" . A_Min										; gets current date time stamp formatting
	send, %TodayDateUniversal2%
	return

:*:[]::
	TodayDateUniversal3 := A_MM . "-" . A_DD . "-" . A_YYYY																	; gets current date time stamp formatting
	send, %TodayDateUniversal3%
	return
	
:*:--::
	send, {space 4}{-}{space}
	return

^WheelUp::
	send, ^{PgUp}
	return

^WheelDown::
	send, ^{PgDn}
	return	
#Ifwinactive
