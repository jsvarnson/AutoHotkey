;--------------------------------------------------
;	Universal Calendar
;--------------------------------------------------
UDateRange1:	
UDateRange:
	gui, submit, nohide ; submit variables
	UDateRgFnd = %UDateRange% ; grab selected date
	UDateRg_Array := StrSplit(UDateRgFnd,"-") ; split into two dates
;--------------- Set Date Var ----------------
	UStrtDate := UDateRg_Array[1] ; grab 1st date
	UEndDate := UDateRg_Array[2] ; grab 2nd date
	UStrtDate1 := UStrtDate ; assign to other var
	UEndDate1 := UEndDate ; assign to other var
;--------------- Format Dates ----------------	
	FormatTime, UStrtDate, %UStrtDate%, MM/dd/yyyy ; format date
	FormatTime, UEndDate, %UEndDate%, MM/dd/yyyy ; format date
	FormatTime, UStrtDate1, %UStrtDate1%, yyyyMMdd ; format date
	FormatTime, UEndDate1, %UEndDate1%, yyyyMMdd ; format date
;--------------- Date Range ------------------	
	UStrtEndCom := " "UStrtDate " - " UEndDate	; format variable
	GuiControl, Text, UDateDisplay, %UStrtEndCom%	; set Edit box
;--------------- Calc Range ------------------		
	;EnvAdd, UStrtDate1, %UEndDate1%, days ; calc date range
	UEndDate1 -= %UStrtDate1%, days
	UDateDistStr := " "UEndDate1 " days"	; format variable
	GuiControl, Text, UDateDist, %UDateDistStr%	; set Edit box
	return
;--------------------------------------------------
;	Universal Notes
;--------------------------------------------------
Save_Universal_Notes:
	Gui, Submit, NoHide
	UFilePath = C:\Users\fm36\OneDrive - Molson Coors Brewing Company\Desktop\UNotes\
	guicontrolget, UTextBox
	UTextBox2 := UTextBox
	if (UTextBox2 = "")																											; if the output is blank => do nothing
	{
		; msgbox, Blank
		return
	}
	else (UTextBox2 != "") 																										; if the output is not blank => continue
	{
		UTitleEnd := InStr(UTextBox2, "`n")-1
		UTitleStart := SubStr(UTextBox2,1,UTitleEnd)
		UFileName = %UFilePath%%UTitleStart%.txt
		FileDelete, %UFileName%
		FileAppend, %UTextBox2%, %UFileName%				; %UNotesFile% -> input file		; `n								; saves Edit box Text as a new file
	}
	return
;---------------------------------------------------------------------------
;	Alarms
;---------------------------------------------------------------------------
;--------------- Alarm 1 ---------------
UAlarm1:							
	gui, submit, nohide
	GuiControl Hide, %URedExPic1%
	GuiControl Show, %UGCheckPic1%
	guicontrolget, UTimeAlarm1
	Unow1 := A_Now
	U_Time_Alarm1 = %UTimeAlarm1%
	UAlarmtarget1 = %A_YYYY%%A_MM%%A_DD%%U_Time_Alarm1%00
	if (UAlarmtarget1 < Unow1)
	{   ; time(today) has passed already, so use time(tomorrow)
		EnvAdd, UAlarmtarget1, 1, d
	}
	EnvSub, UAlarmtarget1, Unow1, Seconds
	SetTimer, UAlarm1Reached, % UAlarmtarget1 * -1000
	TrayTip, %UAlarmName1%, % vUAlarmName1 " set for " UTimeAlarm1,3
	Menu, Tray, Tip, % vUAlarmName1 " set for " UTimeAlarm1
	return	
